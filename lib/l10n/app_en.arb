    "description": "The title of the application"
  },

  "homeTitle": "Home",
  "@homeTitle": {
    "description": "Title for the home screen"
  },

  "settingsTitle": "Settings",
  "@settingsTitle": {
    "description": "Title for the settings screen"
  },

  "welcomeMessage": "Welcome to App Template!",
  "@welcomeMessage": {
    "description": "Welcome message shown on home screen"
  },

  "authLoginTitle": "Login",
  "@authLoginTitle": {
    "description": "Login screen title"
  },

  "authRegisterTitle": "Register",
  "@authRegisterTitle": {
    "description": "Register screen title"
  },

  "authEmailHint": "Email",
