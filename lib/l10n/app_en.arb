    "description": "Register button text"
  },

  "authForgotPassword": "Forgot password?",
  "@authForgotPassword": {
    "description": "Forgot password link text"
  },

  "homeWelcomeMessage": "Hello, {name}!",
  "@homeWelcomeMessage": {
    "description": "Personalized welcome message",
    "placeholders": {
      "name": {
        "type": "String",
        "example": "John"
      }
    }
  },

  "homeItemCount": "{count, plural, =0{No items} =1{1 item} other{{count} items}}",
  "@homeItemCount": {
    "description": "Number of items with plural support",
    "placeholders": {
      "count": {
        "type": "int",
        "example": "5"
      }
    }
  },

  "homeUnreadMessages": "{count, plural, =0{No new messages} =1{1 unread message} other{{count} unread messages}}",
  "@homeUnreadMessages": {
    "description": "Number of unread messages",
    "placeholders": {
      "count": {
        "type": "int",
        "example": "3"
      }
    }
  },

  "commonGenderGreeting": "{gender, select, male{Mr. {name}} female{Ms. {name}} other{{name}}}",
  "@commonGenderGreeting": {
    "description": "Gender-aware greeting",
    "placeholders": {
      "gender": {
        "type": "String",
        "example": "male"
      },
      "name": {
        "type": "String",
        "example": "Smith"
      }
    }
  },

  "commonDateFormat": "Last updated: {date}",
  "@commonDateFormat": {
    "description": "Date display format",
    "placeholders": {
      "date": {
        "type": "DateTime",
        "format": "yMMMd",
        "example": "Jan 1, 2024"
      }
    }
  },

  "commonTimeAgo": "{time, select, now{Just now} minutes{{count} minutes ago} hours{{count} hours ago} days{{count} days ago} other{{time}}}",
  "@commonTimeAgo": {
    "description": "Relative time display",
    "placeholders": {
      "time": {
        "type": "String"
      },
      "count": {
        "type": "int"
      }
    }
  },

  "counterLabel": "You have pushed the button this many times:",
  "@counterLabel": {
    "description": "Label for counter display"
  },

  "counterValue": "{count, plural, =0{zero times} =1{once} =2{twice} other{{count} times}}",
  "@counterValue": {
    "description": "Counter value with plural",
    "placeholders": {
