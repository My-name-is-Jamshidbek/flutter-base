# Development Environment Configuration
# Copy this file to .env.development and adjust values

# API Configuration
API_BASE_URL=http://localhost:3000
API_VERSION=v1

# Timeouts (in seconds)
CONNECT_TIMEOUT=30
RECEIVE_TIMEOUT=30
SEND_TIMEOUT=30

# Retry Configuration
MAX_RETRIES=3

# Feature Flags
ENABLE_LOGGING=true
ENABLE_CRASHLYTICS=false
ENABLE_ANALYTICS=false

# App Info (usually set via build arguments)
APP_NAME=App Template
APP_VERSION=1.0.0
BUILD_NUMBER=1
